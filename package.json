{
  "private": true,
  "name": "@stevenpray/template",
  "version": "1.0.0",
  "description": "General purpose Node.js project template.",
  "license": "MIT",
  "author": "Steven Pray <steven@stevenpray.com>",
  "repository": "github:stevenpray/template",
  "type": "commonjs",
  "files": [
    "bin",
    "lib"
  ],
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "bin": "bin/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": "^16.13.0"
  },
  "scripts": {
    "build": "npm run build:scripts -s && npm run build:types -s",
    "build:scripts": "babel --quiet --extensions='.js,.ts' --ignore='**/*.test.ts' --source-maps --out-dir=lib src",
    "build:types": "tsc --project tsconfig.build.json",
    "clean": "rimraf lib",
    "lint": "npm run lint:docs && npm run lint:scripts && npm run lint:types",
    "lint:docs": "markdownlint .",
    "lint:scripts": "eslint .",
    "lint:types": "tsc",
    "postpack": "npm run clean",
    "prepack": "npm run clean && npm run build",
    "start": "nodemon --no-warnings bin --debug",
    "test": "jest"
  },
