{
  "private": true,
  "name": "@stevenpray/template",
  "version": "1.0.0",
  "description": "General purpose Node.js project template.",
  "license": "MIT",
  "author": "Steven Pray <steven@stevenpray.com>",
  "repository": "github:stevenpray/template",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "files": [
    "bin",
    "lib"
  ],
  "main": "lib",
  "bin": "bin/index.js",
  "engines": {
    "node": ">=12.10"
  },
  "scripts": {
    "build": "npm run build:scripts -s && npm run build:types -s",
    "build:scripts": "babel --quiet --extensions='.js,.ts' --ignore='**/*.test.ts' --source-maps --out-dir=lib src",
    "build:types": "tsc --project tsconfig.build.json",
    "clean": "rimraf lib",
    "lint": "npm run lint:docs -s && npm run lint:scripts -s && npm run lint:types -s",
    "lint:docs": "markdownlint .",
    "lint:scripts": "eslint .",
    "lint:types": "tsc",
    "postpack": "npm run clean -s",
    "prebuild": "npm run clean -s",
    "prepack": "npm run clean -s && npm run build -s",
    "start": "nodemon --no-warnings bin --debug",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.14.7",
    "app-root-path": "3.0.0",
    "core-js": "3.15.1",
    "env-paths": "2.2.1",
    "execa": "5.1.1",
    "lodash": "4.17.21",
    "minimist": "1.2.5",
    "p-timeout": "4.1.0",
    "pino": "6.11.3",
    "pino-pretty": "5.1.0",
    "reflect-metadata": "0.1.13",
    "type-fest": "1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/node": "7.14.7",
